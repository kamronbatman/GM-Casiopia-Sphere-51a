    ENDIF
  ELSEIF <SRC.TARG.ID>=0fae // Closed barrel
    <IF SRC.RESTEST 1 105d> // If they have a spring
      <IF SRC.RESTEST 1 0f0d> // If they have a purple pot
        SRC.CONSUME 1 105d
        SRC.CONSUME 1 0f0d
        SRC.EMOTE setting a trap on a <SRC.TARG.NAME>
        SRC.ANIM 18
        SOUND=004b
        SRC.TARG.TYPE=5510
        SRC.TARG.MOREX=<SRC.TINKERING>
