// ASK TO SELECT VICTIMS HEAD
ONTRIGGER=DCLICK
SAY <SRC.NAME> select the head you wish to turn in... 
TARGET What head do you wish to submit?
RETURN 1

ONTRIGGER=TARGON_ITEM
// CHECK TO MAKE SURE ITS A PLAYERS HEAD ID TYPE?
IF (<SRC.TARG.ID> != 01ce1)
  SRC.MESSAGE Only heads of evil villans may be brought to justice here...
RETURN 1
ENDIF 

// PC PLAYERS HEAD?
IF (<SRC.TARG.LINK> == 04fffffff)
SRC.MESSAGE It must be a player's head!
RETURN 1
ENDIF 

// MUST BE MURDERER 
IF (<SRC.TARG.LINK.KILLS> < 4)
SRC.MESSAGE Only heads of murderers may be brought to justice!
RETURN 1
ENDIF

// TEST FOR A WORTHY HEAD
IF ( !(<SRC.TARG.LINK.FAME> > 2000) )
  SRC.MESSAGE Only heads of truly famous and evil villans are worth a reward... 
  RETURN 1
ELSE
// IT IS A PLAYERS HEAD PROCEED
SRC.NEWITEM=01c18 // create a trophy
SRC.ACT.COLOR=038 // color it gold
SRC.ACT.NAME=Trophy of Justice - issued to <SRC.NAME> for killing <SRC.TARG.LINK.NAME> // Set Trophy ingraving
SRC.ACT.BOUNCE  // Put trophy in their backpack
// START OF STAT LOSS
SOUND=0015d
SRC.TARG.LINK.STR=(<SRC.TARG.LINK.STR>+-1)
SRC.TARG.LINK.DEX=(<SRC.TARG.LINK.DEX>+-1)
SRC.TARG.LINK.INT=(<SRC.TARG.LINK.INT>+-1)
SRC.TARG.LINK.FAME=(<SRC.TARG.LINK.FAME>+(-500))
SRC.FAME=<SRC.FAME>+250
SRC.KARMA=<SRC.KARMA>+250

// START OF BOUNTY SECTION
VAR.KILLS=<SRC.TARG.LINK.KILLS>
VAR.BOUNTY=<VAR.KILLS>*500
IF ( <VAR.KILLS> > 4 ) // start of bounty
  SRC.MESSAGE You receive a bounty of Gold Coins!
  SRC.TARG.LINK.KILLS=(<SRC.TARG.LINK.KILLS>+-2)   // SUBTRACT 2 FROM MURDERERS KILLS
  SRC.NEWITEM=gold
  SRC.ACT.AMOUNT=<VAR.BOUNTY>
  SRC.ACT.BOUNCE
  SRC.MESSAGE You have claimed the bounty!
ELSE
  SRC.MESSAGE There was no bounty on this head!
ENDIF // end of bounty

SRC.TARG.REMOVE // remove the head
SRC.RESEND
RETURN 1
ENDIF

