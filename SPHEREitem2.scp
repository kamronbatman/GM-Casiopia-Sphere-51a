[6034] //Protection Shell
NAME=Protection Shell
COLOR=0786
ID=0fc7
MOREY=10

ONTRIGGER=DCLICK
IF <MOREY>=0
  SRC.SYSMESSAGE=This item has no charges left.
  SOUND=003e
  REMOVE
ELSE
  IF <MOREX>=0
     LINK=<SRC.SERIAL>
     SRC.SYSMESSAGE=This item has bound it self to you.
     SOUND=0214
     SRC.SPELLEFFECT=15 1000
     SRC.SPELLEFFECT=7  1000
     SRC.SPELLEFFECT=36 1000
     MOREX=1
  ELSE
     IF <LINK>!=<SRC.SERIAL>
     SRC.SYSMESSAGE=This item does not belong to you!
  ELSE
     SOUND=0214
     SRC.SPELLEFFECT=15 1000
     SRC.SPELLEFFECT=7  1000
     SRC.SPELLEFFECT=36 1000
     MOREY=<MOREY>+(-1)
  ENDIF
ENDIF
RETURN 1

