SELLVALUE=180-200
MOREX=RAND(10)+5
MOREY=600
MOREZ=10

CATEGORY=!GameMongerCustom
SUBSECTION=Magic Items
DESCRIPTION=Mana Stone

ONTRIGGER=DCLICK
IF ( <MOREX> < 1 ) 
  SRC.SYSMESSAGE The item is void of magical energy, it is but a simple rock now. 
  RETURN 0
ENDIF

IF ( <SRC.MAGERY> < <MOREY> )
  SRC.SYSMESSAGE You are not skilled enough in the mystic art of magery to use this item... 
  RETURN 0
ENDIF

IF ( <SRC.MEDITATION> < <MOREY> )
  SRC.SYSMESSAGE You are not skilled enough in Meditation to use this item... 
  RETURN 0
ENDIF

IF ( <MOREX> > 0 )
MESSAGE Your lifeforce is drained as your mind is refreshed...
SOUND=001fb
MOREX=<MOREX>+(-1)

VAR.POWER=RAND((<MOREY>/10)-10)+10
VAR.MANA=<SRC.MANA>+<VAR.POWER>

  IF (<VAR.MANA> > <SRC.INT>)
    SRC.MANA=<SRC.INT>
  ELSE
    SRC.MANA=<VAR.MANA>
  ENDIF

VAR.DAMAGE=(RAND(<MOREZ>)+5)

// IF PLAYER IS KILLED BY MANA STONE THEN RETURN
  IF ( <VAR.DAMAGE> > <SRC.HITPOINTS> ) 
    SRC.HITPOINTS=0
    SRC.UPDATE
    RETURN 0 
  ENDIF 

  SRC.HITPOINTS=<SRC.HITPOINTS>-(<VAR.DAMAGE>)

// TEST FOR ITEM OUT OF CHARGES AFTER USE AND ADJUST COLOR IF NECESSARY
  IF (<MOREX> == 0)
    COLOR=02bf
    SRC.UPDATE
  ENDIF 

RETURN 1
ENDIF

